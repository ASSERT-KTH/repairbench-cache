 while (iterator.hasNext()) {\n            XYDataset d = (XYDataset) iterator.next();\n            if (d != null) {\n                XYItemRenderer r = getRendererForDataset(d);\n                if (r != null) { // Check if renderer exists\n                    Range range;\n                    if (isDomainAxis) {\n                        range = r.findDomainBounds(d);\n                    } else {\n                        range = r.findRangeBounds(d);\n                    }\n                    result = Range.combine(result, range);\n\n                    // Collect annotations from the renderer\n                    Collection c = r.getAnnotations();\n                    Iterator i = c.iterator();\n                    while (i.hasNext()) {\n                        XYAnnotation a = (XYAnnotation) i.next();\n                        if (a instanceof XYAnnotationBoundsInfo) {\n                            includedAnnotations.add(a);\n                        }\n                    }\n                } else { // No renderer found, use DatasetUtilities\n                    Range range;\n                    if (isDomainAxis) {\n                        range = DatasetUtilities.findDomainBounds(d);\n                    } else {\n                        range = DatasetUtilities.findRangeBounds(d);\n                    }\n                    result = Range.combine(result, range);\n                    // Cannot get annotations if renderer is null\n                }\n            }\n        }\n\n        // Now incorporate the ranges from all collected annotations\n        Iterator it = includedAnnotations.iterator();\n        while (it.hasNext()) {\n            XYAnnotationBoundsInfo xyabi = (XYAnnotationBoundsInfo) it.next();\n            if (xyabi.getIncludeInDataBounds()) {\n                if (isDomainAxis) {\n                    result = Range.combine(result, xyabi.getXRange());\n                } else {\n                    result = Range.combine(result, xyabi.getYRange());\n                }\n            }\n        }\n\n        return result;\n\n    }",
