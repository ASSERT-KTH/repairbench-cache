break;\n                        }\n                        // Ensure reader index is correctly advanced if case didn't consume full extendedLength\n                        if (buf.readerIndex() != dataBlockStart + extendedLength && extendedLength > 0 && buf.readerIndex() > dataBlockStart && buf.readerIndex() < dataBlockStart + extendedLength) {\n                             buf.readerIndex(dataBlockStart + extendedLength); // Adjust if necessary, defensive\n                        }\n                    }\n                    break;\n                case 0xFE:\n                    if (length == 1) {\n                        position.set(Position.KEY_BATTERY_LEVEL, buf.readUnsignedByte());\n                    } else if (length > 1) { // Ensure length is sufficient for mark + potentially other data\n                        int mark = buf.readUnsignedByte(); // Consumes 1 byte from 'length'\n                        if (mark == 0x7C) {\n                            // Alarm data, with battery level as the last byte of the FE block.\n                            // Data for alarms = length (total for FE) - 1 (for mark) - 1 (for battery byte at end)\n                            int alarmDataBytesCount = length - 2;\n                            if (alarmDataBytesCount < 0) alarmDataBytesCount = 0; // Defensive\n                            \n                            int alarmProcessingEndIndex = buf.readerIndex() + alarmDataBytesCount;\n\n                            while (buf.readerIndex() < alarmProcessingEndIndex) {\n                                int extendedType = buf.readUnsignedByte();\n                                int extendedLength = buf.readUnsignedByte(); // Length of this specific alarm's data\n                                int currentAlarmDataStart = buf.readerIndex();\n                                switch (extendedType) {\n                                    case 0x01:\n                                        long alarms = buf.readUnsignedInt();\n                                        if (BitUtil.check(alarms, 0)) { position.set(Position.KEY_ALARM, Position.ALARM_ACCELERATION); }\n                                        if (BitUtil.check(alarms, 1)) { position.set(Position.KEY_ALARM, Position.ALARM_BRAKING); }\n                                        if (BitUtil.check(alarms, 2)) { position.set(Position.KEY_ALARM, Position.ALARM_CORNERING); }\n                                        if (BitUtil.check(alarms, 3)) { position.set(Position.KEY_ALARM, Position.ALARM_ACCIDENT); }\n                                        if (BitUtil.check(alarms, 4)) { position.set(Position.KEY_ALARM, Position.ALARM_TAMPERING); }\n                                        // This case reads 4 bytes. extendedLength must be 4.\n                                        break;\n                                    default:\n                                        buf.skipBytes(extendedLength);\n                                        break;\n                                }\n                                // Ensure reader index is advanced correctly for the alarm block just processed\n                                if (buf.readerIndex() != currentAlarmDataStart + extendedLength && extendedLength > 0 && buf.readerIndex() > currentAlarmDataStart && buf.readerIndex() < currentAlarmDataStart + extendedLength) {\n                                    buf.readerIndex(currentAlarmDataStart + extendedLength);\n                                }\n                            }\n                            // After processing all alarm blocks, if positioned correctly, read battery level from the last byte.\n                            if (buf.readerIndex() == alarmProcessingEndIndex && (length -1 - alarmDataBytesCount == 1) ) { // Ensure 1 byte remains\n                                position.set(Position.KEY_BATTERY_LEVEL, buf.readUnsignedByte());\n                            }\n                        }\n                        // If mark is 0x01 (transparent data) or 0x00 (from test case, now interpreted as not battery)\n                        // or any other mark not explicitly handled to set battery level,\n                        // do nothing here regarding battery level. The outer loop's buf.readerIndex(endIndex)\n                        // will ensure the buffer is advanced past this FE block.\n                    }\n                    break;\n                default:\n                    // buf.skipBytes(length); // Original code implicitly did this via buf.readerIndex(endIndex)\n                    break;\n            }\n            buf.readerIndex(endIndex); // Ensure reader index is set to end of current subtype's block\n        }\n\n        return position;\n    }",
