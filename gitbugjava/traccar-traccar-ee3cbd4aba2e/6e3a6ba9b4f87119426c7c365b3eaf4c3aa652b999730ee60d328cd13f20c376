            case 0x0C:\n                byte[] gyroBytes = new byte[length - 2];\n                buf.readBytes(gyroBytes);\n                if (length == 8) {\n                    try {\n                        int[] gyro = new int[3];\n                        for (int i = 0; i < 3; i++) {\n                            gyro[i] = ((gyroBytes[i * 2] & 0xFF) | ((gyroBytes[i * 2 + 1] & 0xFF) << 8));\n                            if ((gyro[i] & 0x8000)!= 0) {\n                                gyro[i] = -((~gyro[i] + 1) & 0x7FFF);\n                            }\n                        }\n                        position.set(\"tilt\", Arrays.toString(gyro));\n                    } catch (Exception e) {\n                        // handle exception\n                    }\n                }\n                break;\n            default:\n                buf.skipBytes(length);\n                break;\n        }\n    }\n\n    return position;\n}",
